<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <TextView
        android:id="@+id/textView_IdListarLocacao"
        android:layout_width="430dp"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView_ClienteListarLocacao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView_VeiculoListarLocacao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView_DataRetiradaListarLocacao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView_DataDevolucaoListarLocacao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/textView_ValorListarLocacao"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>